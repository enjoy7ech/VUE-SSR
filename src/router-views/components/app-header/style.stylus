#header {
  box-shadow 10px 2px 10px #ddd
  padding 20px 20px
  position absolute
  top 0
  right 0
  left 0
  background #fff
  z-index 999

  .top-banner {
    // position absolute
    // top 0px
    width 40px
    vertical-align middle
  }

  .banner-title {
    margin-left 15px
    font-size 16px
    color #4b5363
    font-weight bold
    display inline-block
    vertical-align middle
  }
}
